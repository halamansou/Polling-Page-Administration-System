@model IEnumerable<Polling_Page_Administration_System.Models.Poll>

<h2>Available Polls</h2>
@foreach (var poll in Model)
{
    <h3>@poll.Title</h3>
    <ul>
        @foreach (var question in poll.Questions)
        {
            <li>
                @question.Text
                <a href="@Url.Action("Answer", new { questionId = question.Id })">Answer</a>
            </li>
        }
    </ul>
}
